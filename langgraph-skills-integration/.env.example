# DeepSeek API 配置
DEEPSEEK_API_KEY=your-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# Skills 路径配置
SKILLS_PATH=../

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=agent.log

# Agent 配置
MAX_EXECUTION_TIME=300
ENABLE_CODE_EXECUTION=true
SANDBOX_MODE=true
MAX_WORKFLOW_STEPS=10

# 性能配置
ENABLE_CACHE=true
CACHE_TTL=3600